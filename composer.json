{
    "name": "static-assets/laravel",
    "description": "Static Assets laravel helper package",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "laravel/framework": "^9.0|^10.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "StaticAssets\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "StaticAssets\\ServiceProvider"
            ]
        }
    },
    "scripts": {
        "cs-fix": "vendor/bin/php-cs-fixer fix"
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
